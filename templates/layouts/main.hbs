<!DOCTYPE html>
<html lang="en" class="no-js">

<head>
    {{{ title }}}
	{{{ meta }}}
    <style type="text/css">{{{data.inlineStyles}}}</style>
</head>

    <body>
        {{{body}}}

        {{> shared/async-css }}


        {{#each data.vendorScripts}}
            <script src="{{this}}"></script>
        {{~/each}}
        {{#each data.remoteScripts}}
            <script src="{{this}}" async></script>
        {{~/each}}
    </body>
</html>
